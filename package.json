{
	"name": "vscode-my",
	"displayName": "My Personal Commands",
	"description": "My personal commands",
	"version": "0.11.1",
	"publisher": "scfrazer",
	"repository": {
		"url": "https://github.com/scfrazer/vscode-my"
	},
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"extensionKind": [
		"ui"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-my.wordLeft",
				"category": "My",
				"title": "Move cursor a word left"
			},
			{
				"command": "vscode-my.wordRight",
				"category": "My",
				"title": "Move cursor a word right"
			},
			{
				"command": "vscode-my.wordPartLeft",
				"category": "My",
				"title": "Move cursor part of a word left"
			},
			{
				"command": "vscode-my.wordPartRight",
				"category": "My",
				"title": "Move cursor part of a word right"
			},
			{
				"command": "vscode-my.deleteWordPartLeft",
				"category": "My",
				"title": "Delete part of a word left"
			},
			{
				"command": "vscode-my.deleteWordPartRight",
				"category": "My",
				"title": "Delete part of a word right"
			},
			{
				"command": "vscode-my.expressionLeft",
				"category": "My",
				"title": "Move cursor an expression left"
			},
			{
				"command": "vscode-my.expressionRight",
				"category": "My",
				"title": "Move cursor an expression right"
			},
			{
				"command": "vscode-my.selectExpressionLeft",
				"category": "My",
				"title": "Select expression left"
			},
			{
				"command": "vscode-my.selectExpressionRight",
				"category": "My",
				"title": "Select expression right"
			},
			{
				"command": "vscode-my.previousParagraph",
				"category": "My",
				"title": "Previous paragraph"
			},
			{
				"command": "vscode-my.nextParagraph",
				"category": "My",
				"title": "Next paragraph"
			},
			{
				"command": "vscode-my.home",
				"category": "My",
				"title": "Home"
			},
			{
				"command": "vscode-my.smartDeleteLeft",
				"category": "My",
				"title": "Smart delete left"
			},
			{
				"command": "vscode-my.smartDeleteRight",
				"category": "My",
				"title": "Smart delete right"
			},
			{
				"command": "vscode-my.selectInsideBrackets",
				"category": "My",
				"title": "Select inside nearest brackets"
			},
			{
				"command": "vscode-my.selectInsideQuotes",
				"category": "My",
				"title": "Select inside nearest quotes"
			},
			{
				"command": "vscode-my.gotoChar",
				"category": "My",
				"title": "Select to char"
			},
			{
				"command": "vscode-my.deleteToChar",
				"category": "My",
				"title": "Select to char"
			},
			{
				"command": "vscode-my.completeCurrentWord",
				"category": "My",
				"title": "Complete current word"
			},
			{
				"command": "vscode-my.completeKeepCompleting",
				"category": "My",
				"title": "Complete current word"
			},
			{
				"command": "vscode-my.addCursorsToLineStarts",
				"category": "My",
				"title": "Add cursors to line starts"
			},
			{
				"command": "vscode-my.addSemicolonToEndOfLine",
				"category": "My",
				"title": "Add a semicolon to end of line"
			},
			{
				"command": "vscode-my.justOneSpace",
				"category": "My",
				"title": "Change multiple spaces to just one"
			},
			{
				"command": "vscode-my.deletePairRight",
				"category": "My",
				"title": "Delete matching pair rightward"
			},
			{
				"command": "vscode-my.gotoLine",
				"category": "My",
				"title": "Go to line"
			},
			{
				"command": "vscode-my.smartTab",
				"category": "My",
				"title": "Smart tab"
			},
			{
				"command": "vscode-my.findInCurrentFile",
				"category": "My",
				"title": "Find in current file"
			},
			{
				"command": "vscode-my.centerCursorLine",
				"category": "My",
				"title": "Center line cursor is on"
			},
			{
				"command": "vscode-my.googleSelection",
				"category": "My",
				"title": "Google Current Selection"
			},
			{
				"command": "vscode-my.oneArgumentPerLine",
				"category": "My",
				"title": "Reformat arguments to be one per line"
			},
			{
				"command": "vscode-my.reselectablePaste",
				"category": "My",
				"title": "Reselectable paste"
			},
			{
				"command": "vscode-my.reselectPreviousPaste",
				"category": "My",
				"title": "Reselect previous paste"
			}
		],
		"keybindings": [
			{
				"key": "ctrl+left",
				"command": "vscode-my.wordLeft",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+right",
				"command": "vscode-my.wordRight",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+shift+,",
				"command": "vscode-my.wordPartLeft",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+shift+.",
				"command": "vscode-my.wordPartRight",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+alt+left",
				"command": "vscode-my.expressionLeft",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+alt+right",
				"command": "vscode-my.expressionRight",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+shift+left",
				"command": "vscode-my.selectExpressionLeft",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+shift+right",
				"command": "vscode-my.selectExpressionRight",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+up",
				"command": "vscode-my.previousParagraph",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+shift+up",
				"command": "vscode-my.previousParagraph",
				"args": {
					"select": true
				},
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+down",
				"command": "vscode-my.nextParagraph",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+shift+down",
				"command": "vscode-my.nextParagraph",
				"args": {
					"select": true
				},
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+backspace",
				"command": "vscode-my.smartDeleteLeft",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "shift+ctrl+backspace",
				"command": "vscode-my.smartDeleteRight",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "alt+backspace",
				"command": "vscode-my.deleteWordPartLeft",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "shift+alt+backspace",
				"command": "vscode-my.deleteWordPartRight",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+delete",
				"command": "vscode-my.smartDeleteRight",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+r",
				"command": "vscode-my.selectInsideBrackets",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+e",
				"command": "vscode-my.selectInsideQuotes",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+m",
				"command": "vscode-my.gotoChar",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+shift+m",
				"command": "vscode-my.gotoChar",
				"args": {
					"select": true
				},
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "alt+m",
				"command": "vscode-my.deleteToChar",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+'",
				"command": "vscode-my.completeCurrentWord",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "alt+shift+i",
				"command": "vscode-my.addCursorsToLineStarts",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+k space",
				"command": "vscode-my.justOneSpace",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+k -",
				"command": "vscode-my.deletePairRight",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+g",
				"command": "vscode-my.gotoLine",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "tab",
				"command": "vscode-my.smartTab",
				"when": "myKeybindingsEnabled && editorTextFocus && !inSnippetMode && !hasSnippetCompletions && !suggestWidgetVisible && !inlineSuggestionVisible"
			},
			{
				"key": "alt+f",
				"command": "vscode-my.findInCurrentFile",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+=",
				"command": "vscode-my.centerCursorLine",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+k ,",
				"command": "vscode-my.oneArgumentPerLine",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+v",
				"command": "vscode-my.reselectablePaste",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "shift+insert",
				"command": "vscode-my.reselectablePaste",
				"when": "myKeybindingsEnabled && editorTextFocus"
			},
			{
				"key": "ctrl+k ctrl+k",
				"command": "vscode-my.reselectPreviousPaste",
				"when": "myKeybindingsEnabled && editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"group": "navigation",
					"command": "vscode-my.googleSelection",
					"when": "editorHasSelection"
				}
			]
		},
		"configuration": {
			"title": "My",
			"properties": {
				"my.enableKeybindings": {
					"type": "boolean",
					"default": true,
					"description": "Enable keybindings"
				},
				"my.todoDecorationDelay": {
					"type": "integer",
					"default": 300,
					"description": "Delay in milliseconds before decorating TODOs"
				}
			}
		},
		"colors": [
			{
				"id": "vscodeMy.fixmeBackground",
				"description": "Background color for FIXME",
				"defaults": {
					"light": "#af0000",
					"dark": "#af0000"
				}
			},
			{
				"id": "vscodeMy.fixmeForeground",
				"description": "Foreground color for FIXME",
				"defaults": {
					"light": "#ffffff",
					"dark": "#ffffff"
				}
			},
			{
				"id": "vscodeMy.todoBackground",
				"description": "Background color for TODO",
				"defaults": {
					"light": "#eeee00",
					"dark": "#eeee00"
				}
			},
			{
				"id": "vscodeMy.todoForeground",
				"description": "Foreground color for TODO",
				"defaults": {
					"light": "#000000",
					"dark": "#000000"
				}
			},
			{
				"id": "vscodeMy.debugBackground",
				"description": "Background color for DEBUG",
				"defaults": {
					"light": "#ffa500",
					"dark": "#ffa500"
				}
			},
			{
				"id": "vscodeMy.debugForeground",
				"description": "Foreground color for DEBUG",
				"defaults": {
					"light": "#000000",
					"dark": "#000000"
				}
			},
			{
				"id": "vscodeMy.noteBackground",
				"description": "Background color for NOTE",
				"defaults": {
					"light": "#27408b",
					"dark": "#27408b"
				}
			},
			{
				"id": "vscodeMy.noteForeground",
				"description": "Foreground color for NOTE",
				"defaults": {
					"light": "#ffffff",
					"dark": "#ffffff"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}
